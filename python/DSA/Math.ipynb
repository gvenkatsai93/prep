   "source": [
    "# https://leetcode.com/problems/powx-n/description/\n",
    "\n",
    "class Solution:\n",
    "    def myPow(self, x: float, n: int) -> float:\n",
    "        if n == 0: return 1\n",
    "        if n < 0 : \n",
    "            n = -n\n",
    "            x = 1 / x\n",
    "        return self.myPow(x*x, n//2) if n%2==0 else x*self.myPow(x*x, n//2)"
   ]
  }
